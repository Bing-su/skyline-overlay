{
  "private": true,
  "name": "skyline-overlay",
  "version": "3.0.0",
  "description": "A horizon FFXIV miniparse overlay built with React & Vite.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "svgr:icons": "npx @svgr/cli -d \"./src/assets/icons\" --ext tsx --svg-props \"className=icon\" --filename-case pascal --icon --typescript \"./assets/icons\"",
    "svgr:jobs": "npx @svgr/cli -d \"./src/assets/jobs\" --ext tsx --svg-props \"className=ijob\" --filename-case kebab --icon --typescript \"./assets/jobs\"",
    "eslint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "typecheck": "tsc",
    "compile": "vite build",
    "cleanup": "node \"./scripts/cleanup.js\"",
    "build": "cross-env NODE_ENV=production run-s typecheck eslint compile cleanup",
    "dev": "cross-env NODE_ENV=development vite",
    "start": "serve"
  },
  "dependencies": {
    "classnames": "~2.3.1",
    "ffxiv-overlay-api": "~4.0.0",
    "i18next": "~21.5.3",
    "lodash": "~4.17.21",
    "mobx": "~6.3.7",
    "mobx-react-lite": "~3.2.2",
    "normalize.css": "~8.0.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-i18next": "~11.14.3",
    "react-transition-group": "~4.4.2"
  },
  "devDependencies": {
    "@svgr/cli": "~5.5.0",
    "@types/lodash": "~4.14.177",
    "@types/node": "~16.11.10",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "~5.4.0",
    "@vitejs/plugin-react-refresh": "~1.3.6",
    "chalk": "~4.1.2",
    "cross-env": "~7.0.3",
    "eslint": "~8.3.0",
    "eslint-plugin-react": "~7.27.1",
    "eslint-plugin-react-hooks": "~4.3.0",
    "glob": "~7.2.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.4.1",
    "sass": "~1.43.5",
    "serve": "~13.0.2",
    "typescript": "~4.5.2",
    "vite": "~2.6.14"
  }
}
