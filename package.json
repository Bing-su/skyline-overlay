{
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "private": true,
  "type": "module",
  "name": "skyline-overlay",
  "version": "3.7.3",
  "description": "A modern customizable horizon FFXIV miniparse overlay.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "eslint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "prettier": "prettier --write .",
    "typecheck": "tsc",
    "bundle": "vite build",
    "serve": "vite",
    "script:genIconIdx": "node \"./scripts/genIconIdx.mjs\"",
    "script:genMeta": "node \"./scripts/genMeta.mjs\"",
    "script:modSource": "node \"./scripts/modSource.mjs\"",
    "script:removeTrash": "node \"./scripts/removeTrash.mjs\"",
    "build": "cross-env NODE_ENV=production run-s script:genIconIdx script:genMeta eslint typecheck bundle script:removeTrash",
    "dev": "cross-env NODE_ENV=development run-s script:genIconIdx script:genMeta serve",
    "start": "serve"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.8.4",
    "@sentry/react": "~7.10.0",
    "@sentry/tracing": "~7.10.0",
    "clsx": "~1.2.1",
    "ffxiv-overlay-api": "~4.4.0",
    "normalize.css": "~8.0.1",
    "react": "~18.2.0",
    "react-colorful": "~5.6.0",
    "react-dom": "~18.2.0",
    "react-redux": "~8.0.2"
  },
  "devDependencies": {
    "@types/node": "~18.7.1",
    "@types/react": "~18.0.17",
    "@types/react-dom": "~18.0.6",
    "@typescript-eslint/eslint-plugin": "~5.33.0",
    "@typescript-eslint/parser": "~5.33.0",
    "@vitejs/plugin-react": "~1.3.2",
    "chalk": "~5.0.1",
    "cross-env": "~7.0.3",
    "eslint": "~8.21.0",
    "eslint-plugin-react": "~7.30.1",
    "eslint-plugin-react-hooks": "~4.6.0",
    "glob": "~8.0.3",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.7.1",
    "rollup-plugin-visualizer": "~5.7.1",
    "sass": "~1.54.4",
    "typescript": "~4.7.4",
    "vite": "~2.9.14",
    "vite-plugin-html-env": "~1.2.4",
    "vite-plugin-pwa": "~0.12.3",
    "vite-plugin-svgr": "~2.2.1",
    "workbox-window": "~6.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}
