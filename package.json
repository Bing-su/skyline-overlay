{
  "name": "skyline-overlay",
  "version": "1.0.0",
  "description": "A horizon FFXIV miniparse overlay built with React & Vite.",
  "scripts": {
    "lint": "eslint --ext \".jsx,.js\" src/",
    "compile": "vite build",
    "cleanup": "node ./cleanup.js",
    "build": "cross-env NODE_ENV=production npm run lint && npm run compile && npm run cleanup",
    "dev": "cross-env NODE_ENV=development vite",
    "serve": "cross-env NODE_ENV=production vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.5.0",
    "classnames": "~2.2.6",
    "ffxiv-overlay-api": "~3.3.0",
    "normalize.css": "~8.0.1",
    "prop-types": "~15.7.2",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-redux": "~7.2.2"
  },
  "devDependencies": {
    "@svgr/rollup": "^5.5.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "sass": "^1.32.7",
    "vite": "2.0.0-beta.69"
  }
}
