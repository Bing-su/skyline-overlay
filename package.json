{
  "private": true,
  "type": "module",
  "name": "skyline-overlay",
  "version": "3.0.0",
  "description": "A modern customizable horizon FFXIV miniparse overlay.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "eslint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "prettier": "prettier --write .",
    "typecheck": "tsc",
    "bundle": "vite build",
    "script:genIconIdx": "node \"./scripts/genIconIdx.js\"",
    "script:updMeta": "node \"./scripts/updMeta.js\"",
    "script:removeSVG": "node \"./scripts/removeSVG.js\"",
    "build": "cross-env NODE_ENV=production run-s script:genIconIdx script:updMeta typecheck eslint bundle script:removeSVG",
    "dev": "cross-env NODE_ENV=development vite",
    "start": "serve"
  },
  "dependencies": {
    "classnames": "~2.3.1",
    "ffxiv-overlay-api": "~4.0.0",
    "i18next": "~21.5.4",
    "lodash": "~4.17.21",
    "mobx": "~6.3.8",
    "mobx-react-lite": "~3.2.2",
    "normalize.css": "~8.0.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-i18next": "~11.14.3",
    "react-transition-group": "~4.4.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "~7.16.0",
    "@svgr/rollup": "~6.0.0",
    "@types/lodash": "~4.14.177",
    "@types/node": "~16.11.11",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@types/react-transition-group": "~4.4.4",
    "@typescript-eslint/eslint-plugin": "~5.5.0",
    "@typescript-eslint/parser": "~5.5.0",
    "@vitejs/plugin-react": "~1.1.0",
    "chalk": "~5.0.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.3.0",
    "eslint-plugin-react": "~7.27.1",
    "eslint-plugin-react-hooks": "~4.3.0",
    "glob": "~7.2.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.5.0",
    "sass": "~1.44.0",
    "serve": "~13.0.2",
    "typescript": "~4.5.2",
    "vite": "~2.6.14",
    "vite-plugin-html-env": "~1.0.3"
  }
}
